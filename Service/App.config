<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<system.serviceModel>
		<services>
			<service name="Service.SensorService">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4002" />
					</baseAddresses>
				</host>
				<endpoint
					address="ServiceSensor"
					binding="netTcpBinding"
					contract="Common.ISensorService" />
			</service>

		</services>
	</system.serviceModel>
</configuration>