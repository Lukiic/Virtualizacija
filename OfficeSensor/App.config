<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<appSettings>
		<add key="csvFile" value="data.csv"/>
	</appSettings>

	<system.serviceModel>
		<client>
			<endpoint name="SensorService"
					  address="net.tcp://localhost:4002/ServiceSensor"
					  binding="netTcpBinding"
					  contract="Common.ISensorService" />
		</client>
	</system.serviceModel>
</configuration>